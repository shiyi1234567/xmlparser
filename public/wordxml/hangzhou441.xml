<Script ID="8edcf375-c1aa-43c9-b925-aac046d16cb2" DisplayName="Word_01_入院记录_04_男性隐藏婚育史，年龄小于14岁女性隐藏月经史" Name="" Object="Document" Scope="Document" Lang="lua" Event="create" >
--替换内容
--男性隐藏月经史\女性小于14岁隐藏月经史
--'Header_V001_L0002'	性别	eg:Header_V001_I003
--'Header_V001_L0003'	年龄	eg:Header_V001_L006
--'男'	男性性别编码	eg:'M'\1 依据项目情况填写
--'女'	女性性别编码	eg:'F'\2 依据项目情况填写
--'S008'	月经史章节	eg:S012
local cmd = luaGetEditorCommand();  
if cmd == nil then      
	return;  
end;   
local objDocument = cmd:GetActiveDocument();   
if objDocument == nil then        
	return; 
end;  
local objGender = objDocument:getElementByPath('Header_V001_L0002');  
if objGender == nil then       
	return;  
end;    
local objValue = objGender:getInstanceData();  
if  objValue == nil then      
	return;  
end;   
local strValueCode= objValue:getData(); 
if  (strValueCode == '男') then      
	cmd:SetSectionVisible(false, 'S008', '');  
end;
local objAge = objDocument:getElementByPath('Header_V001_L0003');  
if objAge == nil then       
	return;  
end;    
local objAgeValue = objAge:getInstanceData();  
if  objAgeValue == nil then      
	return;  
end;   
local strAgeValue= objAgeValue:getData(); 
 
local ageNum=string.match(strAgeValue,&quot;(%d+)岁&quot;);
local len=string.len(ageNum)
if len==1 then
    cmd:SetSectionVisible(false, 'S008', '');  
elseif len==2 then
	local nl=string.sub(ageNum,1,2)
	if (strValueCode == '女')and(tonumber( nl)&lt;14) then
                cmd:SetSectionVisible(false, 'S008', ''); 
    	end;
end;
</Script>
