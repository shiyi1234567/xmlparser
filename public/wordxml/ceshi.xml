function GetScore()
local cmd = luaGetEditorCommand();  
if cmd == nil then      
	return;  
end;   
local objDocument = cmd:GetActiveDocument();   
if objDocument == nil then        
	return; 
end;
-- 初始化数据项
-- 年龄项
local ageItem=0;
-- 体重指数项
local weightItem=0;
-- 活动项
local moveItem=0;
-- 外科手术项
local operationItem=0;
-- 创伤风险项
local traumaItem=0;
-- 特殊风险项
local specialItem=0;
-- 高危疾病项
local illnessItem=0;
-- 年龄项
local obAge = objDocument:getElementByPath('S005_V002_O0001');   
if obAge == nil then        
   
else   
	local obAgeValue = obAge:getInstanceData();   
	if  obAgeValue == nil then  
            
	else	
		ageItem= obAgeValue:getValueCode()-1;
	end;
end;
-- 体重项
local obWeight = objDocument:getElementByPath('S005_V002_O0002');   
if obWeight == nil then        
       
else   
	local obWeightValue = obWeight:getInstanceData();   
	if  obWeightValue == nil then     
        
	else	
		weightItem= obWeightValue:getValueCode()-1;
	end;
end;
--  活动项
local obMove = objDocument:getElementByPath('S005_V002_O0003');   
if obMove == nil then        
    
else   
	local obMoveValue = obMove:getInstanceData();   
	if  obMoveValue == nil then   
 
	else	
		moveItem= obMoveValue:getValueCode()-1;
	end;
end;
-- 外科手术项
local obOperation = objDocument:getElementByPath('S005_V002_O0004');   
if obOperation == nil then        
    
else   
	local obOperationValue = obOperation:getInstanceData();   
	if  obOperationValue == nil then   
 
	else	
		local tempStr=obOperationValue:getValueCode();
		if tempStr==&quot;1&quot; then
			operationItem=0;
		else
			local tempNum=tonumber(tempStr)
			operationItem=(tempNum-(tempNum%10))/10;
		end;
	end;
end;
-- 多选项的算分
-- 创伤风险
local trauma=&quot;&quot;;
local obTrauma = objDocument:getElementByPath('S005_V002_M0001');   
if obTrauma == nil then
else   
    local obTraumaValue = obTrauma:getInstanceData();   
    if  obTraumaValue == nil then            
    else
        trauma= obTraumaValue:getData();
		-- IFxModuleCenter.MsgBox('结果', '['..trauma..']', IFxModuleCenter.MB_OK);
		if string.find(trauma,&quot;头部受伤&quot;) then
		traumaItem=traumaItem+0
		end;
		if string.find(trauma,&quot;胸部受伤&quot;) then
		traumaItem=traumaItem+1
		end;
		if string.find(trauma,&quot;脊柱受伤&quot;) then
		traumaItem=traumaItem+2
		end;
		if string.find(trauma,&quot;盆骨受伤&quot;) then
		traumaItem=traumaItem+3
		end;
		if string.find(trauma,&quot;下肢受伤&quot;) then
		traumaItem=traumaItem+4
		end;
        -- IFxModuleCenter.MsgBox('结果', '['..traumaItem..']', IFxModuleCenter.MB_OK);
		trauma=&quot;&quot;
	end;
end;
-- 特殊风险项
local special=&quot;&quot;;
local obSpecial = objDocument:getElementByPath('S005_V002_M0002');   
if obSpecial == nil then
else   
    local obSpecialValue = obSpecial:getInstanceData();   
    if  obSpecialValue == nil then            
    else
        special= obSpecialValue:getData();
		if string.find(special,&quot;20-35岁&quot;) then
		specialItem=specialItem+0
		end;
		if string.find(special,&quot;35岁以上&quot;) then
		specialItem=specialItem+1
		end;
		if string.find(special,&quot;激素治疗&quot;) then
		specialItem=specialItem+2
		end;
		if string.find(special,&quot;怀孕/产褥期&quot;) then
		specialItem=specialItem+3
		end;
		if string.find(special,&quot;血栓形成&quot;) then
		specialItem=specialItem+4
		end;
		special=&quot;&quot;
	end;
end;
-- 高危疾病项
local illness=&quot;&quot;;
local obIllness = objDocument:getElementByPath('S005_V002_M0003');   
if obIllness == nil then
else   
    local obIllnessValue = obIllness:getInstanceData();   
    if  obIllnessValue == nil then            
    else
        illness= obIllnessValue:getData();
		if string.find(illness,&quot;溃疡性结肠炎&quot;) then
		illnessItem=illnessItem+1
		end;
		if string.find(illness,&quot;红细胞增多症&quot;) then
		illnessItem=illnessItem+2
		end;
		if string.find(illness,&quot;静脉曲张&quot;) then
		illnessItem=illnessItem+3
		end;
		if string.find(illness,&quot;慢性心脏病&quot;) then
		illnessItem=illnessItem+3
		end;
		if string.find(illness,&quot;急性心肌梗死&quot;) then
		illnessItem=illnessItem+4
		end;
		if string.find(illness,&quot;恶性肿瘤&quot;) then
		illnessItem=illnessItem+5
		end;
		if string.find(illness,&quot;脑血管疾病&quot;) then
		illnessItem=illnessItem+6
		end;
		if string.find(illness,&quot;静脉栓塞病史&quot;) then
		illnessItem=illnessItem+7
		end;
		illness=&quot;&quot;
	end;
end;
local TotalScore=1*(tonumber(ageItem)+tonumber(weightItem)+tonumber(moveItem)+tonumber(operationItem)+tonumber(traumaItem)+tonumber(specialItem)+tonumber(illnessItem))
--精确2位小数
cmd: SetElementDisplayName(tostring(TotalScore),'S005_V002_L0016','');
end;
